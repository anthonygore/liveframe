{
  "name": "liveframe",
  "version": "0.1.0",
  "description": "A simple javascript library to help with manipulation of an iframe. It works by making edits in a dummy DOM before inserting it into the iframe and re-rendering.",
  "main": "dist/liveframe.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdir -p ./dist && ./node_modules/.bin/babel liveframe.js -o dist/liveframe.js --presets es2015"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonygore/liveframe.git"
  },
  "keywords": [
    "iframe"
  ],
  "author": "Anthony Gore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anthonygore/liveframe/issues"
  },
  "homepage": "https://github.com/anthonygore/liveframe#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0"
  }
}
